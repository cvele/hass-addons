{
  "name": "Mongo v4",
  "version": "4.4.18",
  "slug": "mongo4",
  "description": "A Mongo addon for Home Assistant",
  "arch": [
    "aarch64"
  ],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "27017/tcp": 27017
  },
  "map": [
    "config:rw",
    "ssl"
  ],
  "options": {
    "log_level": "info",
    "auth": true,
    "username": "admin",
    "password": "password"
  },
  "schema": {
    "log_level": "str",
    "auth": "bool",
    "username": "str",
    "password": "str"
  },
  "image": "ghcr.io/cvele/hass-mongo4",
  "icon": "icon.png"
}
