{
    "name": "Miniflux",
    "version": "2.2.3-3",
    "slug": "miniflux",
    "description": "A Miniflux addon for Home Assistant",
    "init": true,
    "arch": [
        "aarch64"
    ],
    "startup": "services",
    "boot": "auto",
    "options": {
        "ADMIN_USERNAME": "admin",
        "ADMIN_PASSWORD": null,
        "RUN_MIGRATIONS": true,
        "DATABASE_URL": "postgres://miniflux:secret@db/miniflux?sslmode=disable"
    },
    "ports": {
        "8080/tcp": 8080
    },
    "schema": {
        "ADMIN_USERNAME": {
            "type": "string"
        },
        "ADMIN_PASSWORD": {
            "type": "password",
            "nullable": true
        },
        "RUN_MIGRATIONS": {
            "type": "boolean"
        },
        "DATABASE_URL": {
            "type": "string",
            "format": "uri"
        }
    },
    "webui": "http://[HOST]:[PORT:8080]",
    "image": "ghcr.io/cvele/hass-miniflux",
    "icon": "icon.png"
}
