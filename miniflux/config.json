{
    "name": "Miniflux",
    "version": "2.2.3-9",
    "slug": "miniflux",
    "description": "A Miniflux addon for Home Assistant",
    "init": false,
    "arch": [
        "aarch64"
    ],
    "startup": "services",
    "boot": "auto",
    "options": {
        "ADMIN_USERNAME": "admin",
        "ADMIN_PASSWORD": null,
        "RUN_MIGRATIONS": true,
        "DATABASE_URL": "postgres://miniflux:secret@db/miniflux?sslmode=disable"
    },
    "ports": {
        "8080/tcp": 8080
    },
    "schema": {
        "ADMIN_USERNAME": "str",
        "ADMIN_PASSWORD": "password",
        "RUN_MIGRATIONS": "bool",
        "DATABASE_URL": "url"

    },
    "webui": "http://[HOST]:[PORT:8080]",
    "image": "ghcr.io/cvele/hass-miniflux",
    "icon": "icon.png"
}
