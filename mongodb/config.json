{
    "name": "MongoDB",
    "version": "4.4.8-1",
    "slug": "mongodb",
    "description": "A MongoDB addon for Home Assistant based on Unofficial Mongo for RBPI",
    "arch": [
        "aarch64"
    ],
    "startup": "services",
    "boot": "auto",
    "apparmor": false,
    "options": {
        "MONGO_INITDB_ROOT_USERNAME": "homeassistant",
        "MONGO_INITDB_ROOT_PASSWORD": "homeassistant"
    },
    "ports": {
        "27017/tcp": 27017
    },
    "schema": {
        "MONGO_INITDB_ROOT_USERNAME": "str",
        "MONGO_INITDB_ROOT_PASSWORD": "password"
    },
    "image": "ghcr.io/cvele/hass-mongodb",
    "map": {
        "config": {
            "source": "/mnt/data/mongo",
            "target": "/data/db",
            "type": "rw"
        }
    },
    "icon": "icon.png"
}
